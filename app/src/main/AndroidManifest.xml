<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.malumukendi.assignment6activities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivateActivity"
            android:label="Go back to Login" />

        <!--suppress AndroidDomInspection, AndroidDomInspection, AndroidDomInspection -->
        <service
            android:name=".service.serviceImpl.CustomerServiceImpl"
            android:enabled="true"
            android:exported="true" />

    </application>

</manifest>